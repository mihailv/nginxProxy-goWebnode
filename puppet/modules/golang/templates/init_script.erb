#!/bin/bash

case $1 in
    start)
        echo "Starting web server <%= app %>."
		<%= path %>/bin/<%= app %> &
        ;;
    stop)
        echo "Stopping web server <%= app %>."
        sudo kill $(sudo lsof -t -i: <%= webnode_port %>)
        ;;
    *)
        echo "About web server <%= app %>."
        echo $"Usage $0 {start|stop}"
        exit 1
esac
exit 0
